    "json5": "^1.0.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
